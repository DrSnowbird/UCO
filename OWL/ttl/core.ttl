###  #Controlled-vocabulary

<#Controlled-vocabulary> rdf:type owl:Class .



###  http://uco.org/core#Annotation

:Annotation rdf:type owl:Class ;
            
            rdfs:subClassOf :Assertion ,
                            [ rdf:type owl:Class ;
                              owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :refs ;
                                                     owl:onClass :Uco-object ;
                                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                   ]
                                                 )
                            ] .



###  http://uco.org/core#Array-of-string

:Array-of-string rdf:type owl:Class .



###  http://uco.org/core#Assertion

:Assertion rdf:type owl:Class ;
           
           rdfs:subClassOf :Uco-object ,
                           [ rdf:type owl:Class ;
                             owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty :statement ;
                                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onDataRange xsd:BasicString
                                                  ]
                                                )
                           ] .



###  http://uco.org/core#Facet

:Facet rdf:type owl:Class .



###  http://uco.org/core#Item

:Item rdf:type owl:Class ;
      
      rdfs:subClassOf :Uco-object .



###  http://uco.org/core#Reference

:Reference rdf:type owl:Class ;
           
           rdfs:subClassOf :Uco-object .



###  http://uco.org/core#Uco-object

:Uco-object rdf:type owl:Class ;
            
            rdfs:subClassOf [ rdf:type owl:Class ;
                              owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :created_by_ref ;
                                                     owl:onClass <http://uco.org/core/identity#Identity> ;
                                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                   ]
                                                 )
                            ] ,
                            [ rdf:type owl:Class ;
                              owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :spec_version ;
                                                     owl:onClass <#Controlled-vocabulary> ;
                                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                   ]
                                                 )
                            ] ,
                            [ rdf:type owl:Class ;
                              owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :id ;
                                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onDataRange xsd:Identifier
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :id ;
                                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onDataRange xsd:Identifier
                                                   ]
                                                 )
                            ] ,
                            [ rdf:type owl:Class ;
                              owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :name ;
                                                     owl:onClass <#Controlled-vocabulary> ;
                                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                   ]
                                                 )
                            ] ,
                            [ rdf:type owl:Class ;
                              owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :created_time ;
                                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onDataRange xsd:Timestamp
                                                   ]
                                                 )
                            ] .



